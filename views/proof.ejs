<!DOCTYPE html>
<html>
  <head>
    <title>RSQIS</title>
    <%- include('./common/link'); %>
    <%- include('./common/navbar',{nav:{
      home:"",
      road:"active",
      work:"",
      user:data.user,
      username:data.username   
    }}); %>
    <style>
    .col-md-4{
        margin-bottom:2%; 
    }
    </style>
    </style>
  </head>
  <body>
    div class="container" style="margin-top:5%;margin-bottom:5%;">
    <!-- Card -->
    <h3
      class="text-center"
      style="margin-top: 5%;text-decoration: underline;"
    >
      Proof
    </h3>
    <br />
    <div class="row">
      <br />
        <%if(data.result==null){%>
          <h4 class="text-center" style="margin: auto;">No data found.</h4>
          <%} else {%>
             <% for(let x=0;x<data.result.length;x++){ %> 
     
            <div class="col col-md-4">
        <div class="card">
          <!-- Card image -->
          <img
            class="card-img-top"
            src="/images/<%=data.result[x].path%>"
            alt="Card image cap"
          />

          <!-- Card content -->
          <div class="card-body">
            <!-- Title -->
            <h4 class="card-title"><a><%=data.result[x].Email%></data.result></a></h4>
            <!-- Text -->
            <p class="card-text">
             <%=data.result[x].description%>
            </p>
            <%const d=new Date(data.result[x].Timestamp)%>
            <p><b>Date:</b><i><%=d%></i></p>
           </div>
        </div>

        <!-- Card -->
      </div>
      <%} %>
     <%}%>
     
    </div>
    <br><br>
  </div>
  

    <%- include('./common/footer'); %>
<%- include('./common/script'); %>
<script>
  function change(){
    document.querySelector("#frm").submit();
  }
  $(function () {
  $('[data-toggle="tooltip"]').tooltip()
})

</script>  

</body>

</html>
